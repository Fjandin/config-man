{
  "name": "@fjandin/config-man",
  "version": "1.1.0",
  "description": "Config manager",
  "main": "index.js",
  "author": "Ren√© Bischoff <rene.bischoff@gmail.com>",
  "license": "MIT",
  "private": false,
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/"
    ],
    "report-dir": "./coverage",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "scripts": {
    "peer": "yarn add --peer aws-sdk",
    "tslint": "tslint -c tslint.json './src/**/*.ts' --project tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "CM_test1_test1a=env CM_env_unknown=test nyc mocha --opts ./test/mocha.opts -r ts-node/register",
    "test:watch": "nyc mocha --opts ./test/mocha-w.opts -r ts-node/register",
    "build": "rm -rf dist; tsc; cp package.json ./dist; cp .npmrc ./dist"
  },
  "dependencies": {
    "flat": "4.1.0"
  },
  "devDependencies": {
    "@types/chai-as-promised": "7.1.0",
    "@types/chai-datetime": "0.0.32",
    "@types/flat": "0.0.28",
    "@types/mocha": "5.2.7",
    "@types/mock-require": "2.0.0",
    "@types/node": "12.6.2",
    "@types/sinon": "7.0.13",
    "@types/sinon-chai": "3.2.2",
    "aws-sdk-mock": "4.5.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.5.0",
    "mocha": "6.1.4",
    "mock-require": "3.0.3",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "aws-sdk": "2.287.0"
  }
}
